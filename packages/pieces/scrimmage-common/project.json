{
  "name": "scrimmage-common",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/pieces/scrimmage-common/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/pieces/scrimmage-common",
        "main": "packages/pieces/scrimmage-common/src/index.ts",
        "tsConfig": "packages/pieces/scrimmage-common/tsconfig.lib.json",
        "assets": ["packages/pieces/scrimmage-common/*.md"],
        "buildableProjectDepsInPackageJsonType": "dependencies",
        "updateBuildableProjectDepsInPackageJson": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/pieces/scrimmage-common/**/*.ts"]
      }
    }
  },
  "tags": []
}
